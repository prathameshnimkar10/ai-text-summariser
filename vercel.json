{
    "version": 2,
    "builds": [
        {
            "src": "api/summarise.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/summarise",
            "dest": "/api/summarise.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}